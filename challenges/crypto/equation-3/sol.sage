### Part 1

def mod(x, modulus):
    r = x
    for a in modulus:
        r = r % a
    return r

def powerPoly(a, b, modulus):
    res = 1
    while (b > 0):
        if (b & 1): res = mod(res * a, modulus)
        a = mod(a * a, modulus)
        b = b // 2
    return res

F.<m1, m2> = Zmod(N)[]

ff = ff(m1 = m1, m2 = m2)
